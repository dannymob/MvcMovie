@using CodeProject.ViewModel
@model FileUploadViewModel
@{
    Layout = "~/Views/Shared/MyLayout.cshtml";
}

@section TitleSection{ Bulk Upload }

@section ContentBody {
    <div id="fileUp">
        <a class="btn btn-default" href="~/Views/Employee/Index.cshtml">Back</a>

        <form action="~/BulkUpload/Upload" method="post" enctype="multipart/form-data">
            <input class="btn btn-default" type="file" name="fileUpload" value="" />
            <input class="btn btn-success" type="submit" name="name" value="Upload" />
        </form>
    </div>    
}